    "hepburn": "^1.2.0",
    "html-minifier": "^4.0.0",
    "ioredis": "^4.22.0",
    "lodash": "^4.17.20",
    "mime-type-check": "^2.0.0",
    "moment-timezone": "^0.5.33",
    "mongo-cursor-pagination-alt": "^1.0.0",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.15",
    "qs": "^6.10.1",
    "ts-mongoose": "^0.0.24",
    "unofficial-jisho-api": "^2.0.13",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  }
}
