    "@apidevtools/swagger-parser": "10.1.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.61",
    "@swc/jest": "0.2.26",
    "@types/base64-url": "2.2.0",
    "@types/cheerio": "0.22.31",
    "@types/compression": "1.7.0",
    "@types/consolidate": "0.14.0",
    "@types/cors": "2.8.13",
    "@types/ejs": "3.1.2",
    "@types/eslint": "8.40.0",
    "@types/eslint-config-prettier": "6.11.0",
    "@types/express": "4.17.17",
