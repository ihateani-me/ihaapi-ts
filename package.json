{
  "name": "ihaapi-ts",
  "version": "2.5.2",
  "description": "A maybe-pretty-fast API endpoint mainly made for VTuber stuff.",
  "main": "src/main.ts",
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "swc src -d build",
    "copy-assets": "node tools/copyAssets",
    "swaggerdoc": "node tools/buildSwagger",
    "swaggercheck": "node tools/validateSwagger",
    "build": "npm-run-all clean swaggerdoc copy-assets compile build:cleanup",
    "build:cleanup": "node tools/cleanupBuild",
    "dev": "npm-run-all swaggerdoc dev:start",
    "dev:start": "nodemon",
    "prod": "npm-run-all build prod:start",
    "prod:start": "node build/main.js",
    "prodpriv": "npm-run-all build prodpriv:start",
    "prodpriv:start": "node build/main_private.js",
    "test": "jest",
    "lint": "eslint src/",
    "testall": "npm-run-all test swaggerdoc swaggercheck",
    "tsync": "typesync && npm install",
    "syncupload": "node tools/uploader"
  },
  "engines": {
    "node": ">= 12.x"
  },
  "prepush": {
    "run": "syncupload"
  },
  "prepush.silent": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihateani-me/ihaapi-ts.git"
  },
  "keywords": [
    "vtuber",
    "api",
    "express",
    "steam",
    "hltb",
    "u2",
    "saucenao",
    "iqdb",
    "ascii2d",
    "nhentai"
  ],
  "author": "N4O",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ihateani-me/ihaapi-ts/issues"
  },
  "homepage": "https://github.com/ihateani-me/ihaapi-ts#readme",
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@swc/cli": "^0.1.36",
    "@swc/core": "^1.2.52",
    "@swc/jest": "^0.1.2",
    "@types/base64-url": "^2.2.0",
    "@types/cheerio": "^0.22.23",
    "@types/compression": "^1.7.0",
    "@types/consolidate": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/ejs": "^3.0.5",
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.7",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/fs-extra": "^9.0.6",
    "@types/hepburn": "^1.2.0",
    "@types/html-minifier": "^4.0.0",
    "@types/ioredis": "^4.19.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mongodb": "^3.6.6",
    "@types/mongoose": "^5.10.3",
    "@types/nodemon": "^1.19.0",
    "@types/qs": "^6.9.6",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "8.1.0",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "swagger-jsdoc": "^6.0.2",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.4",
    "typesync": "^0.8.0"
  },
  "dependencies": {
    "altair-express-middleware": "^3.1.0",
    "apollo-cache-control": "^0.12.0",
    "apollo-datasource-rest": "^0.10.0",
    "apollo-server-cache-redis": "^1.2.3",
    "apollo-server-core": "^2.22.2",
    "apollo-server-express": "^2.20.0",
    "axios": "^0.21.1",
    "base64-url": "^2.3.3",
    "bson": "^4.2.3",
    "cheerio": "1.0.0-rc.3",
    "compression": "^1.7.4",
    "consolidate": "^0.16.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "form-data": "^3.0.0",
    "graphql": "^15.5.0",
